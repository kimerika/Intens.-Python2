
car_speeds = [270, 263, 285, 279, 308, 288, 305, 300, 263, 320, 275, 261, 272, 311, 290, 289, 293, 280, 325, 287, 295, 293, 319, 281, 325, 300, 271, 310, 326, 326, 298, 272, 279, 318, 284, 307, 298, 272, 303, 281]

fastest_car_speed = max(car_speeds)

first_fastest_car_index = car_speeds.index(fastest_car_speed)

print("Порядковый номер первого самого быстрого автомобиля: ", first_fastest_car_index + 1)


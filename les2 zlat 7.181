# Первая кость
first_bone = numbers[0] // 10, numbers[0] % 10

# Флаг для проверки соответствия правила игры
flag = True

# Проверка последовательности чисел на соответствие правилам игры
for number in numbers[1:]:
    left, right = first_bone

    # Проверяем соответствие правилам игры
    if left == number % 10:
        first_bone = number // 10, number % 10
    elif right == number % 10:
        first_bone = number % 10, number // 10
    else:
        # Если правило нарушено, меняем значение флага
        flag = False
        break

# Вывод результата
if flag:

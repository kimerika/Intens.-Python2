marks = [2, 4, 3, 5, 2, 3, 4, 3, 2, 5, 4, 2, 3, 4, 5, 3, 2, 2, 4, 3, 3, 4, 2, 5, 3] # массив данных об оценках 25 учеников по химии

failing_grades = 0 # переменная для хранения количества неуспевающих учеников

for i in marks: # проходим по каждой оценке в массиве
    if i < 3: # если оценка меньше 3, то увеличиваем счетчик неуспевающих
        failing_grades += 1

print(f'Количество неуспевающих учеников по химии: {failing_grades}') # выводим результат

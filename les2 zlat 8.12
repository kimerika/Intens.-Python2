
a = float(input("Введите вещественное число a: "))
s = 0  # переменная для хранения текущей суммы

# цикл для перебора значений n от 2 до достаточно большого числа (в данном случае 10^6)
for n in range(2, 10**6):
    s += 1/n  # добавляем очередное слагаемое в текущую сумму
    if 1 + s > a:  # если условие выполнилось
        print(n)  # печатаем значение переменной n


# Определить массив целых чисел
numbers = [12, 27, 35, 48, 55, 64, 75, 82, 97, 105]

# Инициализировать счетчики количества четных элементов и элементов, оканчивающихся на 5
even_count = 0
ends_with_five_count = 0

# Вычислить количество четных элементов и элементов, оканчивающихся на 5
for number in numbers:
    if number % 2 == 0:
        even_count += 1
    if number % 10 == 5:
        ends_with_five_count += 1

# Вывести результаты
print("Количество четных элементов массива:", even_count)
print("Количество элементов, оканчивающихся на цифру 5:", ends_with_five_count)

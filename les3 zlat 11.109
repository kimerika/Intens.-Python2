
arr = [1, 2, 2, 5, 7, 7, 7, 9, 9, 10, 11, 12, 12, 12, 15, 16, 16, 19, 19, 20]
count = 0
diff_count = 1

for i in range(1, len(arr)):
    if arr[i] == arr[i-1]:
        count += 1
    if arr[i] != arr[i-1]:
        diff_count += 1

print("Количество пар одинаковых соседних элементов:", count)
print("Количество различных чисел в массиве:", diff_count)

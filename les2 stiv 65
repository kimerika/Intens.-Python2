
print("Таблица соотношения температур в градусах Цельсия и Фаренгейта")
print("--------------------------------------------------------------------")
print("  °C      °F")
print("--------------")

for celsius in range(0, 101, 10):
    fahrenheit = (celsius * 9/5) + 32
    print("  {}      {}".format(celsius, fahrenheit))

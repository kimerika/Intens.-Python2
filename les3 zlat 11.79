# Создаем массив с результатами игр
results = [3, 2, 1, 3, 1, 2, 3, 2, 2, 1, 3, 1, 1, 1, 2, 2, 3, 3, 2, 1]

# Инициализируем переменные для хранения количества выигрышей, ничьих и проигрышей
wins = 0
draws = 0
losses = 0

# Проходимся по всем элементам массива с результатами
for result in results:
    if result == 3:
        wins += 1
    elif result == 1:
        draws += 1
    elif result == 2:
        losses += 1

# Выводим результаты
print("Выигрыши:", wins)
print("Ничьи:", draws)
print("Проигрыши:", losses)

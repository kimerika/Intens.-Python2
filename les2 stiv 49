
year = int(input("Введите год рождения: "))

animals = ["Обезьяна", "Петух", "Собака", "Свинья", "Крыса", "Бык", "Тигр", "Кролик", "Дракон", "Змея", "Лошадь", "Овца"]
start_year = 1924

index = (year - start_year) % 12
animal = animals[index]

print("Ваше животное по китайскому гороскопу:", animal)

 
notes = {
    1: "Григорий Распутин",
    2: "Александр Сергеевич Пушкин",
    5: "Михаил Юрьевич Лермонтов",
    10: "Дмитрий Иванович Менделеев",
    20: "Петр Ильич Чайковский",
    50: "Николай Иванович Пирогов",
    100: "Владимир Ильич Ленин",
    200: "Петр I",
    500: "Петр II"
}

note_input = int(input("Введите номинал банкноты: "))
if note_input in notes:
    print("На банкноте номиналом", note_input, "рублей изображен", notes[note_input])
else:
    print("Банкноты номиналом", note_input, "рублей не существует")

# задаем значения переменных
a = "строка1"
b = "строка2"
c = "строка3"

# меняем значения переменных с помощью дополнительной переменной
temp = b
b = c
c = a
a = temp

# выводим результат
print("Новое значение переменной а:", a)
print("Новое значение переменной b:", b)
print("Новое значение переменной c:", c)

# меняем значения переменных без дополнительной переменной
a, b, c = c, a, b

# выводим результат
print("Новое значение переменной а:", a)
print("Новое значение переменной b:", b)
print("Новое значение переменной c:", c)

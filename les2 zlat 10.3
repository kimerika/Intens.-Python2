
import random

# получаем случайные натуральные числа t и l, не превосходящие 20
t = random.randint(1, 20)
l = random.randint(1, 20)

# выводим t и l на экран
print("t =", t)
print("l =", l)

# запрашиваем значения a и b с клавиатуры
a = int(input("Введите значение a: "))
b = int(input("Введите значение b: "))

# генерируем l случайных целых чисел в диапазоне от a до b включительно
print("Случайные целые числа:")
for i in range(l):
    num = random.randint(a, b)
    print(num)

# генерируем t случайных вещественных чисел в диапазоне от 0 до b
print("Случайные вещественные числа:")
for i in range(t):
    num = random.uniform(0, b)
    print(num)

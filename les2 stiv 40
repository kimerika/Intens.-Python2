
noise_level = int(input("Введите уровень шума в децибелах: "))

if noise_level == 130:
    print("Уровень шума соответствует старту ракеты")
elif noise_level == 120:
    print("Уровень шума соответствует выстрелу из пистолета")
elif noise_level == 110:
    print("Уровень шума соответствует мотоциклу")
elif noise_level == 100:
    print("Уровень шума соответствует газонокосилке")
elif noise_level == 90:
    print("Уровень шума соответствует грузовику")
elif noise_level == 80:
    print("Уровень шума соответствует громкому разговору")
elif noise_level == 70:
    print("Уровень шума соответствует обычному разговору")
elif noise_level == 60:
    print("Уровень шума соответствует нормальной библиотечной тишине")
elif noise_level == 50:
    print("Уровень шума соответствует тихому шепоту")
else:
    if noise_level > 130:
        print("Уровень шума превышает старт ракеты")
    elif noise_level > 120:
        print("Уровень шума превышает выстрел из пистолета")
    elif noise_level > 110:
        print("Уровень шума превышает мотоцикл")
    elif noise_level > 100:
        print("Уровень шума превышает газонокосилку")
    elif noise_level > 90:
        print("Уровень шума превышает грузовик")
    elif noise_level > 80:
        print("Уровень шума превышает громкий разговор")
    elif noise_level > 70:
        print("Уровень шума превышает обычный разговор")
    elif noise_level > 60:
        print("Уровень шума превышает нормальную библиотечную тишину")
    elif noise_level > 50:
        print("Уровень шума превышает тихий шепот")
    else:
        print("Введенное значение не соответствует уровню шума")

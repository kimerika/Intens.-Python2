n = int(input('Введите количество домов на улице: ')) # вводим количество домов

total_odd = 0 # переменная для хранения количества жителей на нечетной стороне
total_even = 0 # переменная для хранения количества жителей на четной стороне

for i in range(1, n+1): # цикл от 1 до n включительно, чтобы проверить количество жителей на каждой стороне

    residents = int(input(f'Введите количество жителей в доме номер {i}: ')) # вводим количество жителей

    if i % 2 == 0: # если номер дома четный, то добавляем жителей на четную сторону
        total_even += residents
    else: # иначе добавляем жителей на нечетную сторону
        total_odd += residents

# сравниваем общее количество жителей на нечетной и четной стороне и выводим результат
if total_odd > total_even:
    print('На нечетной стороне улицы проживает больше жителей')
elif total_even > total_odd:
    print('На четной стороне улицы проживает больше жителей')
else:
    print('Количество жителей на обеих сторонах улицы равное')

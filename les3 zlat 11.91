# задаем исходный массив масс в килограммах
mass = [70, 85, 98, 102, 110, 95, 100, 125, 130, 98, 101, 104, 115, 120, 92, 105, 115, 96, 112, 121, 98, 99, 108, 115, 90]

# инициализируем счетчик
count = 0

# перебираем элементы массива
for elem in mass:
    if elem > 100:
        # если масса больше 100 кг - увеличиваем счетчик
        count += 1

# выводим результат на экран
print("Количество полных людей (с массой более 100 кг) в группе равно:", count)



def is_palindrome(string):
    length = len(string)
    for i in range(length // 2):
        if string[i] != string[length - i - 1]:
            return False
    return True

string = input("Введите строку: ")

if is_palindrome(string):
    print("Строка является палиндромом")
else:
    print("Строка не является палиндромом")

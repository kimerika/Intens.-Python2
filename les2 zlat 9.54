
def gcd(a, b):
    """
    Функция для нахождения НОД двух чисел
    """
    if a == 0:
        return b
    return gcd(b % a, a)

# Перебираем все числа от 2 до 7 для знаменателя d
for d in range(2, 8):
    # Перебираем все числа от 1 до d-1 для числителя n
    for n in range(1, d):
        # Находим НОД числителя и знаменателя
        g = gcd(n, d)
        # Если НОД равен 1, то дробь несократима
        if g == 1:
            # Печатаем дробь вида n/d
            print(f"{n}/{d}")

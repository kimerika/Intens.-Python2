
prices = [10.5, 8.2, 15.3, 7.1, 12.0, 9.5, 18.7, 14.6, 10.1, 11.0, 16.8, 6.7, 13.2, 20.0, 8.5, 17.0, 10.9, 9.7, 11.8, 14.1]

average_price = sum(prices) / len(prices)

count = 0
for price in prices:
    if price < average_price:
        count += 1

print("Количество видов товара, имеющих стоимость меньшую, чем средняя стоимость всех видов товара:", count)
